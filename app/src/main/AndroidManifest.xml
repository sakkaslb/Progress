<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.massoluciones.com.progress">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="progress.massoluciones.com.progress.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="progress.massoluciones.com.progress.MainActivity" android:label="Progress" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.LoginActivity" android:label="Progress" android:theme="@style/Theme.FloatingWindow.Popup" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.GoalsActivity" android:label="Progress"    android:theme="@style/Theme.MyFancyTheme" android:parentActivityName="progress.massoluciones.com.progress.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.MemoryActivity" android:label="Progress"    android:theme="@style/Theme.MyFancyTheme" android:parentActivityName="progress.massoluciones.com.progress.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.ProgressActivity" android:label="Progress"    android:theme="@style/Theme.MyFancyTheme" android:parentActivityName="progress.massoluciones.com.progress.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.SocialActivity" android:label="Progress"    android:theme="@style/Theme.MyFancyTheme" android:parentActivityName="progress.massoluciones.com.progress.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.SnapshotActivity" android:label="Progress"    android:theme="@style/Theme.MyFancyTheme" android:parentActivityName="progress.massoluciones.com.progress.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.WeightActivity" android:label="Progress"    android:theme="@style/Theme.FloatingWindow.Popup" android:screenOrientation="portrait"/>
        <activity android:name="progress.massoluciones.com.progress.ListadoCheckinsActivity" android:label="Progress"    android:theme="@style/Theme.MyFancyTheme" android:parentActivityName="progress.massoluciones.com.progress.SnapshotActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>
